{
  "name": "scheduledapp",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=20.19.1",
    "npm": ">=9.6.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint app/ components/ lib/ --max-warnings 0",
    "lint:fix": "eslint app/ components/ lib/ --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:python": "pytest",
    "test:email": "node scripts/test-email.js",
    "verify:email": "node scripts/verify-email-config.js",
    "verify:deploy": "node scripts/verify-deployment.js",
    "verify:validation": "node scripts/verify-validation.js",
    "verify:auth": "node scripts/verify-auth.js",
    "validate": "./scripts/validate-before-push.sh",
    "validate:quick": "npm run lint && npm run typecheck && npm run build",
    "test:all": "npm run lint && npm run typecheck && npm run test && npm run test:e2e",
    "test:ci": "npm run lint && npm run typecheck && npm run test:coverage && npm run test:e2e",
    "test:a11y": "pa11y-ci",
    "test:visual": "playwright test --grep @visual",
    "test:perf": "lhci autorun",
    "test:security": "npm audit --audit-level=high",
    "test:docker": "docker-compose -f docker-compose.test.yml down -v && docker-compose -f docker-compose.test.yml up --build test-unit test-e2e",
    "test:docker:all": "docker-compose -f docker-compose.test.yml down -v && docker-compose -f docker-compose.test.yml up --build --exit-code-from test-unit",
    "test:docker:unit": "docker-compose -f docker-compose.test.yml down -v && docker-compose -f docker-compose.test.yml up --build test-unit",
    "test:docker:e2e": "docker-compose -f docker-compose.test.yml down -v && docker-compose -f docker-compose.test.yml up --build test-e2e",
    "test:docker:a11y": "docker-compose -f docker-compose.test.yml up --build test-a11y",
    "test:docker:visual": "docker-compose -f docker-compose.test.yml up --build test-visual",
    "test:docker:perf": "docker-compose -f docker-compose.test.yml up --build test-perf",
    "build:pa11y-config": "node scripts/generate-pa11y-config.js",
    "audit:fix": "npm audit fix",
    "dependencies:check": "npx npm-check-updates",
    "dependencies:update": "npx npm-check-updates -u && npm install",
    "postinstall": "node scripts/post-install.js",
    "shrinkwrap": "npm shrinkwrap",
    "dedupe": "npm dedupe",
    "clean:install": "rm -rf node_modules package-lock.json && npm install",
    "fix:deps": "node scripts/fix-dependencies.js",
    "validate:deps": "node scripts/validate-dependencies.js",
    "lock:validate": "node scripts/validate-dependency-locks.js",
    "lock:check": "node scripts/check-dependency-status.js",
    "lock:audit": "node scripts/audit-dependencies.js",
    "lock:report": "node scripts/generate-dependency-report.js",
    "lock:fix": "node scripts/fix-dependency-issues.js",
    "lock:update": "node scripts/update-dependencies-safe.js",
    "lock:security": "npm audit --audit-level=moderate && node scripts/audit-dependencies.js",
    "maintenance:weekly": "node scripts/weekly-maintenance.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "2.49.4",
    "chalk": "5.4.1",
    "chart.js": "4.4.9",
    "date-fns": "4.1.0",
    "motion": "12.9.1",
    "next": "15.4.6",
    "next-auth": "5.0.0-beta.29",
    "next-themes": "0.4.6",
    "openai": "4.100.0",
    "react": "19.1.0",
    "react-chartjs-2": "5.3.0",
    "react-dom": "19.1.0",
    "react-icons": "5.5.0",
    "react-intersection-observer": "9.16.0",
    "react-use": "17.6.0",
    "sharp": "0.34.1",
    "zod": "3.24.4"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@next/eslint-plugin-next": "15.4.6",
    "@playwright/test": "1.54.2",
    "@testing-library/jest-dom": "6.6.4",
    "@testing-library/react": "14.2.1",
    "@testing-library/user-event": "14.5.2",
    "@types/cors": "2.8.18",
    "@types/express": "5.0.2",
    "@types/helmet": "0.0.48",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.30",
    "@types/pino": "7.0.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/testing-library__jest-dom": "5.14.9",
    "@types/uuid": "10.0.0",
    "autoprefixer": "10.4.16",
    "dotenv": "16.5.0",
    "eslint": "9.39.2",
    "globals": "17.3.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "postcss": "8.4.31",
    "resend": "4.4.1",
    "tailwindcss": "3.3.5",
    "typescript": "5.4.5",
    "typescript-eslint": "8.39.0"
  },
  "pythonDependencies": {
    "pytest": "7.4.0",
    "pytest-cov": "4.1.0",
    "pytest-mock": "3.11.1",
    "pytest-xdist": "3.3.1"
  },
  "resolutions": {
    "postcss": "8.4.31"
  },
  "overrides": {
    "cookie": "1.0.2"
  }
}
