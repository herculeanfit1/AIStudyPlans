    "openai": "4.100.0",
    "react": "18.3.1",
    "react-chartjs-2": "5.3.0",
    "react-dom": "18.3.1",
    "react-icons": "5.5.0",
    "react-intersection-observer": "9.16.0",
    "react-tsparticles": "2.12.2",
    "react-use": "17.6.0",
    "sharp": "0.34.1",
    "three": "0.159.0",
    "tsparticles": "2.12.0",
    "tsparticles-slim": "2.12.0",
    "zod": "3.24.4"
  },
  "devDependencies": {
    "@playwright/test": "1.42.1",
