{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/_next/*",
      "/images/*",
      "/favicon.ico",
      "/robots.txt",
      "/sitemap.xml"
    ]
  },
  "routes": [
    {
      "route": "/app",
      "redirect": "https://app.aistudyplans.com",
      "statusCode": 302
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  },
  "globalHeaders": {
    "cache-control": "must-revalidate, max-age=3600",
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "X-XSS-Protection": "1; mode=block"
  }
} 