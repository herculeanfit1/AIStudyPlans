{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "mimeTypes": {
    ".json": "application/json"
  },
  "routes": [
    {
      "route": "/api/waitlist",
      "allowedRoles": ["anonymous"],
      "rewrite": "https://aistudyplans-function.azurewebsites.net/api/waitlist"
    },
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/login",
      "allowedRoles": ["anonymous"],
      "rewrite": "/login.html"
    },
    {
      "route": "/logout",
      "allowedRoles": ["anonymous"],
      "rewrite": "/logout.html"
    },
    {
      "route": "/admin/direct-login*",
      "redirect": "/admin/ms-login",
      "statusCode": 301
    },
    {
      "route": "/admin/emergency*",
      "redirect": "/admin/ms-login",
      "statusCode": 301
    },
    {
      "route": "/admin/login*",
      "redirect": "/admin/ms-login",
      "statusCode": 301
    }
  ]
} 