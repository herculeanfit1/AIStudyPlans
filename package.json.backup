{
  "name": "scheduledapp",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:python": "pytest",
    "test:email": "node scripts/test-email.js",
    "test:all": "npm run lint && npm run typecheck && npm run test && npm run test:e2e",
    "test:ci": "npm run lint && npm run typecheck && npm run test:coverage && npm run test:e2e",
    "test:a11y": "pa11y-ci",
    "test:visual": "playwright test --grep @visual",
    "test:perf": "lhci autorun",
    "test:security": "npm audit --audit-level=high",
    "test:docker": "docker-compose -f docker-compose.test.yml up --build test-unit test-e2e",
    "test:docker:all": "docker-compose -f docker-compose.test.yml up --build --exit-code-from test-unit",
    "test:docker:unit": "docker-compose -f docker-compose.test.yml up --build test-unit",
    "test:docker:e2e": "docker-compose -f docker-compose.test.yml up --build test-e2e",
    "test:docker:a11y": "docker-compose -f docker-compose.test.yml up --build test-a11y",
    "test:docker:visual": "docker-compose -f docker-compose.test.yml up --build test-visual",
    "test:docker:perf": "docker-compose -f docker-compose.test.yml up --build test-perf",
    "build:pa11y-config": "node scripts/generate-pa11y-config.js"
  },
  "dependencies": {
    "@auth/core": "^0.35.1",
    "@react-three/drei": "^9.92.1",
    "@react-three/fiber": "^8.15.12",
    "@supabase/supabase-js": "^2.49.4",
    "chart.js": "^4.4.9",
    "framer-motion": "^12.9.1",
    "next": "^14.2.28",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-tsparticles": "^2.12.2",
    "react-use": "^17.6.0",
    "sharp": "^0.34.1",
    "three": "^0.158.0",
    "tsparticles": "^2.12.0",
    "tsparticles-slim": "^2.12.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.5.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.31",
    "resend": "^4.4.1",
    "tailwindcss": "^3.3.5",
    "typescript": "^5"
  },
  "pythonDependencies": {
    "pytest": "^7.4.0",
    "pytest-cov": "^4.1.0",
    "pytest-mock": "^3.11.1",
    "pytest-xdist": "^3.3.1"
  }
}
