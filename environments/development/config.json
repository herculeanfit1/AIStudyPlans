{
  "environment": "development",
  "api": {
    "baseUrl": "https://dev-aistudyplans.azurewebsites.net/api",
    "timeout": 10000
  },
  "features": {
    "feedback": true,
    "analytics": false,
    "debugMode": true,
    "waitlist": true,
    "feedbackCampaign": true
  },
  "logging": {
    "level": "debug",
    "enableConsole": true,
    "enableRemote": false
  },
  "deployment": {
    "containerRegistry": "ghcr.io",
    "imageName": "aistudyplans",
    "appServiceName": "dev-aistudyplans"
  },
  "env": {
    "NEXT_PUBLIC_SUPABASE_URL": "${NEXT_PUBLIC_SUPABASE_URL}",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "${NEXT_PUBLIC_SUPABASE_ANON_KEY}",
    "RESEND_API_KEY": "${RESEND_API_KEY}",
    "EMAIL_FROM": "noreply-dev@aistudyplans.com",
    "NEXT_PUBLIC_APP_URL": "http://localhost:3000"
  }
} 