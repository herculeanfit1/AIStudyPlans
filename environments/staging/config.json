{
  "environment": "staging",
  "api": {
    "baseUrl": "https://staging-aistudyplans.azurewebsites.net/api",
    "timeout": 8000
  },
  "features": {
    "feedback": true,
    "analytics": true,
    "debugMode": false,
    "waitlist": true,
    "feedbackCampaign": true
  },
  "logging": {
    "level": "info",
    "enableConsole": true,
    "enableRemote": true
  },
  "deployment": {
    "containerRegistry": "ghcr.io",
    "imageName": "aistudyplans",
    "appServiceName": "staging-aistudyplans"
  },
  "env": {
    "NEXT_PUBLIC_SUPABASE_URL": "${NEXT_PUBLIC_SUPABASE_URL}",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "${NEXT_PUBLIC_SUPABASE_ANON_KEY}",
    "RESEND_API_KEY": "${RESEND_API_KEY}",
    "EMAIL_FROM": "noreply-staging@aistudyplans.com",
    "NEXT_PUBLIC_APP_URL": "https://staging-aistudyplans.azurewebsites.net"
  }
} 